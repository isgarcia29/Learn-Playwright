{
  "name": "learn-playwright",
  "version": "1.0.0",
  "description": "automated software testing with playwright",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tests:e2e": "playwright test --config=e2e.config.ts --project=Chromium",
    "tests:e2eFirefox": "playwright test --config=e2e.config.ts --project=Firefox",
    "tests:api": "playwright test --config=api.config.ts --project=Chromium",
    "tests:apiReport": "playwright test --config=api.config.ts --project=Chromium --reporter=html",
    "tests:apiReportFirefox": "playwright test --config=api.config.ts --project=Firefox --reporter=html"
  },
  "author": "Israel Garcia",
  "license": "ISC",
  "dependencies": {
    "@playwright/test": "^1.34.3",
    "prettier": "^2.8.8"
  }
}
